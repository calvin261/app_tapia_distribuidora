{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "outputDirectory": ".next"
      }
    }
  ],
  "regions": ["iad1"],
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "functions": {
    "src/app/**/*.ts": {
      "runtime": "nodejs18.x"
    },
    "src/app/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
